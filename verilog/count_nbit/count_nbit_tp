#! /opt/homebrew/Cellar/icarus-verilog/12.0/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x11f007eb0 .scope module, "count_nbit_tp" "count_nbit_tp" 2 3;
 .timescale -12 -12;
P_0x11f006780 .param/l "STEP" 0 2 7, +C4<00000000000000000000001111101000>;
v0x11f01d030_0 .var "clk", 0 0;
v0x11f01d0d0_0 .net "count", 3 0, L_0x11f01d250;  1 drivers
v0x11f01d180_0 .var "rst", 0 0;
S_0x11f008020 .scope module, "count_nbit" "count_nbit" 2 12, 3 1 0, S_0x11f007eb0;
 .timescale -12 -12;
    .port_info 0 /INPUT 1 "CLK";
    .port_info 1 /INPUT 1 "RST";
    .port_info 2 /OUTPUT 4 "COUNT_N";
P_0x11f008d60 .param/l "N_BIT" 0 3 2, +C4<00000000000000000000000000000100>;
L_0x11f01d250 .functor BUFZ 4, v0x11f01cf80_0, C4<0000>, C4<0000>, C4<0000>;
v0x11f00cdc0_0 .net "CLK", 0 0, v0x11f01d030_0;  1 drivers
v0x11f01ce50_0 .net "COUNT_N", 3 0, L_0x11f01d250;  alias, 1 drivers
v0x11f01cef0_0 .net "RST", 0 0, v0x11f01d180_0;  1 drivers
v0x11f01cf80_0 .var "count_n", 3 0;
E_0x11f006830 .event posedge, v0x11f00cdc0_0;
    .scope S_0x11f008020;
T_0 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x11f01cf80_0, 0, 4;
    %end;
    .thread T_0;
    .scope S_0x11f008020;
T_1 ;
    %wait E_0x11f006830;
    %load/vec4 v0x11f01cef0_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x11f01cf80_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x11f01cf80_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x11f01cf80_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x11f007eb0;
T_2 ;
    %delay 500, 0;
    %load/vec4 v0x11f01d030_0;
    %inv;
    %store/vec4 v0x11f01d030_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x11f007eb0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11f01d030_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11f01d180_0, 0, 1;
    %delay 15000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11f01d180_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11f01d180_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x11f01d180_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11f01d180_0, 0, 1;
    %delay 1000, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x11f007eb0;
T_4 ;
    %vpi_call 2 23 "$monitor", $stime, " clk=%b rst=%b count=%h", v0x11f01d030_0, v0x11f01d180_0, v0x11f01d0d0_0 {0 0 0};
    %end;
    .thread T_4;
